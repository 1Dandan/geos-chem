!  NOTES:
!  (1 ) Changed RCS ID tag comment character from "C" to "!$" to allow freeform
!        compilation.  Also added & continuation characters in column 73
!        to allow header files to be included in F90 freeform files.
!        (bmy, 6/25/02)
!  (2 ) Fix for LINUX - remove & from column 73 (bmy, 6/27/03)

      INTEGER NFIELDS2,NSEASON_last2,NCOHCS,NJVREAD,                    
     &        NOH,NNCOUNT,NNEW,MNCOUNT,MNEW,NO3READ,                    
     &        NTALDT,LNCOUNT,LNEW,NCLIMATOLOGY,NCLIMATOLOGY2,           
     &        NCLIMATOLOGY3,LMN_last
      PARAMETER(NFIELDS2=12,NJVREAD=65,NO3READ=65)
      PARAMETER(NCOHCS=3)
      INTEGER Rcount(IIPAR,JJPAR,LLPAR)
      REAL*8 BBIJ(IIPAR,JJPAR,LLPAR,NFIELDS2),                          
     &       o3up(IIPAR,JJPAR,LLPAR)
      REAL*8 krate(IIPAR,JJPAR,LLPAR),                                  
     &       SUMISOPCO(IIPAR,JJPAR),SUMMONOCO(IIPAR,JJPAR)
      REAL*8 GCO(IIPAR,JJPAR,LLPAR),GO3(IIPAR,JJPAR,LLPAR),             
     &       GCH4(IIPAR,JJPAR,LLPAR)
      REAL*8 GISOP(IIPAR,JJPAR,LLPAR)
      REAL*8 Tavg(IIPAR,JJPAR,LLPAR),Pavg(IIPAR,JJPAR,LLPAR),           
     &       Ravga(IIPAR,JJPAR,LLPAR),Ravgb(IIPAR,JJPAR,LLPAR),         
     &       Ravg(IIPAR,JJPAR,LLPAR),Wavg(IIPAR,JJPAR,LLPAR)
      REAL*8 STTTOGCO(IIPAR,JJPAR,LLPAR),                               
     &       STTTOPPB(IIPAR,JJPAR,LLPAR)
      REAL*8 bairdens(IIPAR,JJPAR,LLPAR)
      REAL*8 totco(IIPAR,JJPAR,LLPAR),Pco(IIPAR,JJPAR,LLPAR)
      REAL*8 SUMCO(IIPAR,JJPAR,LLPAR,NCOHCS),AVGNO
      REAL*8 CO_prod(JJPAR,LLPAR),CO_loss(JJPAR,LLPAR)
      PARAMETER(AVGNO=6.022E23)
      COMMON /COb1/ o3up,BBIJ,bairdens
      COMMON /COb2/ NNCOUNT,NNEW,MNCOUNT,MNEW,NSEASON_last2,            
     &              LMN_last
      COMMON /COb3/ Rcount,Pco 
      COMMON /COb4/ Tavg,Pavg,Ravg,Ravga,Ravgb,Wavg
      COMMON /COb5/ NTALDT,SUMISOPCO,SUMMONOCO
      COMMON /COb7/ LNCOUNT,LNEW

      COMMON /COb8/ CO_prod,CO_loss

      INTEGER NREFL,NREFLCOL
      PARAMETER (NREFL=226,NREFLCOL=5)
      REAL*8 OPTDEPTH(NREFL),RFLC(NREFL,NREFLCOL)
      COMMON /COb9/OPTDEPTH,RFLC

      REAL*8 FASTJO3(31,18,12),FASTJT(41,18,12),                        
     &    FASTJZ(IIPAR,JJPAR,LLPAR)
      COMMON/OTHERCLIM/ FASTJO3,FASTJT,FASTJZ
      
      INTEGER NNMONTHS,NNLATITUDES,NNALTITUDES,NNSEAS,NLAYO3
      PARAMETER(NNMONTHS=12,NNLATITUDES=16,NNALTITUDES=31,              
     & NNSEAS=4,NLAYO3=9)
      REAL*8 stratclimO3(NNLATITUDES,NNALTITUDES,NNMONTHS),             
     & zstarstratclim(NNALTITUDES),pstratclim(NNALTITUDES)
      REAL*8 tropclimO3(IIPAR,JJPAR,NLAYO3,NNSEAS),                     
     & ptropclim(NLAYO3)
      DATA ptropclim /1000.,900.,800.,700.,500.,300.,200.,150.,100./

      COMMON/STRATCLIM/ stratclimO3,zstarstratclim,pstratclim
      COMMON/TROPCLIM/ tropclimO3

      ! TOMS O3 column interannual variation
      REAL*8 ctm4x5month(IIPAR,JJPAR)
      COMMON/TOMSO3/ctm4x5month
